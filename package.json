{
  "name": "idna-uts46-hx",
  "description": "A UTS #46 processing algorithm for IDNA2008 strings",
  "version": "4.0.0",
  "private": false,
  "main": "dist/uts46.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/hexonet/idna-uts46",
  "repository": "github:hexonet/idna-uts46",
  "bugs": "https://github.com/hexonet/idna-uts46/issues",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "email": "kschwarz@hexonet.net"
    }
  ],
  "keywords": [
    "hexonet",
    "unicode",
    "idn",
    "idna",
    "domain"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "**/*.spec.js",
      "idna-map.js"
    ]
  },
  "xo": {
    "prettier": true,
    "globals": [],
    "ignores": [
      "!@(vendor|coverage|.nyc_output|.github|node_modules|pkg|build)/**",
      "!**/*.@(all|min).js"
    ]
  },
  "unicodeVersion": "14.0.0",
  "scripts": {
    "prettier": "prettier -u --write \"**/*\"",
    "test": "npm run test-COVERAGE --silent",
    "test-MOCHA": "./node_modules/.bin/_mocha $(find ./test -type f -name '*.spec.js')",
    "test-COVERAGE": "cross-env NODE_ENV=development nyc npm run test-MOCHA --silent",
    "lint": "xo && npm run lint-python",
    "lint-python": "./scripts/pep8fix.sh",
    "prepare": "husky install",
    "build:unicode-tables": "esr scripts/build-unicode-tables.ts 14.0.0 "
  },
  "dependencies": {
    "punycode": "^2.1.1"
  },
  "devDependencies": {
    "@hexonet/semantic-release-github-npm-config": "^2.0.4",
    "@semantic-release/exec": "^6.0.2",
    "@types/axios": "^0.14.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "esbuild-runner": "^2.2.1",
    "fancy-log": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "prettier-config-xo": "^2.0.0",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.8.1",
    "xo": "^0.48.0"
  },
  "lint-staged": {
    "*": "prettier -u --write"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": true
  }
}
