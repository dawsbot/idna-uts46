{
  "name": "idna-uts46-hx",
  "description": "A UTS #46 processing algorithm for IDNA2008 strings",
  "version": "4.0.0",
  "private": false,
  "main": "dist/uts46.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/hexonet/idna-uts46",
  "repository": "github:hexonet/idna-uts46",
  "bugs": "https://github.com/hexonet/idna-uts46/issues",
  "keywords": [
    "unicode",
    "idn",
    "idna",
    "domain"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "xo": {
    "prettier": true,
    "globals": [],
    "ignores": [
      "!@(vendor|coverage|.nyc_output|.github|node_modules|pkg|build)/**",
      "!**/*.@(all|min).js"
    ]
  },
  "unicodeVersion": "14.0.0",
  "scripts": {
    "prettier": "prettier -u --write \"**/*\"",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "mocha",
    "test:coverage": "cross-env NODE_ENV=development nyc npm run test --silent",
    "lint": "eslint src",
    "prepare": "husky install",
    "build:unicode-tables": "esr scripts/build-unicode-tables.ts 14.0.0"
  },
  "dependencies": {
    "punycode": "^2.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/axios": "^0.14.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "esbuild-runner": "^2.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^10.0.5",
    "fancy-log": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "ts-node": "^10.8.1"
  },
  "lint-staged": {
    "*": "prettier -u --write"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": true
  }
}
